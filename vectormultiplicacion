#include "stdafx.h"
#include<iostream>
#include "conio.h"

using namespace std;

int main(){
	int numero[]= {1,2,3,4,5};
	int suma = 0;
	int multiplicacion=1;

	for(int i=0; i<5; i++){
		suma=suma + numero[i];
		multiplicacion=multiplicacion * numero[i];


	}

	cout<<"La suma de los elementos del vector es:"<<suma<<endl;

	cout<<"La suma de los elementos del vector es:"<<multiplicacion<<endl;


	getch();
	return 0;

}


